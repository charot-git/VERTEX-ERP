                              <Label fx:id="companyEmailLabel1" layoutX="20.0" layoutY="10.0" style="-fx-text-fill: #ff0000;" textFill="RED">
                                 <VBox.margin>
                                    <Insets left="10.0" />
                                 </VBox.margin>
                              </Label>
                           </children>
                        </VBox>
                        <Button fx:id="chooseLogoButton" maxWidth="-Infinity" minWidth="150.0" mnemonicParsing="false" style="-fx-font-size: 10; -fx-max-width: 100; -fx-max-height: 20;" styleClass="text-input" text="choose pic" />
                     </children>
                  </HBox>
                  <HBox spacing="20.0">
                     <children>
                        <Button fx:id="confirmButton" mnemonicParsing="false" text="Confirm">
                           <styleClass>
                              <String fx:value="button" />
                              <String fx:value="text-input" />
                           </styleClass>
                        </Button>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="15.0" left="30.0" right="30.0" top="15.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0" />
   </children>
</AnchorPane>
